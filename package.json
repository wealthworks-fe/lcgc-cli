    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "ejs": "^3.0.1",
    "execa": "^4.0.0",
    "inquirer": "^7.0.1",
    "random-color": "^1.0.1",
    "shelljs": "^0.8.3"
  }
}
